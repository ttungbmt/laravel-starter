{
    "name": "ttungbmt/livewire-form",
    "description": "A dynamic, responsive Laravel Livewire form component with realtime validation, file uploads, array fields, and more.",
    "keywords": [
        "laravel",
        "livewire",
        "table"
    ],
    "homepage": "https://github.com/ttungbmt/livewire-form",
    "license": "MIT",
    "authors": [
        {
            "name": "Truong Thanh Tung",
            "email": "ttungbmt@gmail.com"
        }
    ],
    "require": {
        "kdion4891/laravel-livewire-forms": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "ttungbmt\\Livewire\\Form\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ttungbmt\\Livewire\\Form\\Providers\\FormServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
